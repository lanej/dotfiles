"*":
  "Remote-FTP": {}
  "atom-alignment": {}
  "atom-beautify":
    c:
      default_beautifier: "clang-format"
    cpp:
      default_beautifier: "clang-format"
    general:
      _analyticsUserId: "9916bd6f-639e-4dab-8387-453bd57c2133"
    ruby:
      disabled: true
      indent_size: 2
      rubocop_path: "/usr/local/bin/rubocop"
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
    defaultProvider: "Fuzzy"
    useCoreMovementCommands: false
  core:
    audioBeep: false
    closeEmptyWindows: false
    disabledPackages: [
      "linter-rubocop"
      "welcome"
      "language-gfm"
      "autocomplete-ruby"
    ]
    projectHome: "/Users/jlane/p"
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "atom-material-syntax-dark"
    ]
    useCustomTitleBar: true
  editor:
    fontSize: 10
    invisibles: {}
    preferredLineLength: 120
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    zoomFontWhenCtrlScrolling: true
  "ever-notedown":
    openPreviewInSplitPane: false
    pulledContentInSplitPane: true
  "exception-reporting":
    userId: "c141f735-ebf0-4522-4185-982b368094dd"
  "git-plus":
