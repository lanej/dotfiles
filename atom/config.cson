    panelDisplayMode: "closed"
    runCoverageOnSave: true
    test:
      focusPanelIfTestsFail: false
      runTestsOnSave: false
  "highlight-selected":
    onlyHighlightWholeWords: true
  "linter-rubocop":
    disableWhenNoConfigFile: true
  "linter-ui-default": {}
  minimap:
    adjustAbsoluteModeHeight: true
    displayCodeHighlights: false
  "remote-sync":
    foldLogPanel: true
  "rubocop-auto-correct":
    correctFile: true
    notification: false
    onlyFixesNotification: false
    rubocopCommandPath: "bundle exec rubocop"
  "tester-go":
    focusPanelIfTestsFail: false
  "travis-ci-status":
    personalAccessToken: "d6400007a92d1dea548968f3a868e1518165ec21"
    useTravisCiPro: true
  welcome:
    showOnStartup: false
".erb.html.text":
  editor:
    preferredLineLength: 120
    scrollPastEnd: true
    softWrapHangingIndent: 120
".evnd.gfm.html.inline.markdown.mathjax.source.text":
